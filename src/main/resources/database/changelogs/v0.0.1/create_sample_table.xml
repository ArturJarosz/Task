<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="add sample data to database" author="liquibase">
        <createTable tableName="SAMPLE_TABLE">
            <column name="ST_ID" type="int(11)" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_ST_ID"/>
            </column>
            <column name="ST_NAME" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>